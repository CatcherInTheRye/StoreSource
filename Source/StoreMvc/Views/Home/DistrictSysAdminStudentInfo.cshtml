@model PCS.DataRepository.DataContracts.StudentForm
@{
    ViewBag.Title = "DistrictSysAdminStudents";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="intelli-manager">
    <div data-role="page" class="wrapper-content">
        <div id="column-left">
            <a class="logo" href="#" title="">
                <img src="/images/logo-dictrict.png" width="229" height="120" alt="" />
            </a>
            <div style="text-align: center;">
                <a href="@(Url.Action("Logout"))" title="">Logout</a></div>
            <ul class="intelli-mainmenu">
                <li><a href="@(Url.Action("DistrictSysAdmin"))" title="">Settings</a></li>
                <li><a href="@(Url.Action("DistrictSysAdminSchools"))" title="">Schools</a></li>
                <li><a href="@(Url.Action("DistrictSysAdminStaff"))" title="">Staff</a></li>
                <li><a href="@(Url.Action("DistrictSysAdminStudents"))" title="">Students</a></li>
            </ul>
            <div class="intelli-copy">
                <span>Developed by</span> <a href="http://www.intellimedia.ca/" title="Visit Intellimedia inc.">
                    <img src="/images/logoIntellimedia.png" with="131" height="23" alt="" />
                </a>
            </div>
        </div>
        <!-- /left column -->
        <div id="column-right">
            <div data-role="content">
                <div class="intelli-content">
                    <div class="intelli-main-content-header ui-bar ui-bar-a">
                        <a href="javascript:;" class="icoMenu" id="togglepanel"><span>Menu</span></a>
                        <!--a class="ui-btn ui-nodisc-icon ui-btn-inline ui-btn-icon-left ui-btn-b ui-icon-bars ui-btn-icon-notext" href="javascript:;" id="togglepanel">Panel</a-->
                        <h3>
                            District and School Students
                        </h3>
                    </div>
                    <div class="intelli-main-content ui-body ui-body-a">
                        <p>
                            <a target="_self" rel="external" href="@(Url.Action("DistrictSysAdminStudents"))" class="intelli-btn-white ui-link">
                                &lt; Back To List</a>
                        </p>
                        <p>
                            <table width="100%" cellspacing="0" cellpadding="0" border="0" class="table1">
                                <colgroup>
                                    <col width="30%" />
                                    <col width="70%" />
                                </colgroup>
                                <tr>
                                    <td>
                                        Last Name:
                                    </td>
                                    <td>@(Model.LastName)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        First Name:
                                    </td>
                                    <td>@(Model.FirstName)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Middle Name:
                                    </td>
                                    <td>@(Model.MiddleName)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        School:
                                    </td>
                                    <td>@(Model.School)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Student Number:
                                    </td>
                                    <td>@(Model.StudentNumber)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Gender:
                                    </td>
                                    <td>@(Model.GenderS)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Dob:
                                    </td>
                                    <td>@(Model.Dob.ToString("MMMM dd, yyyy", System.Globalization.CultureInfo.InvariantCulture))
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Grade:
                                    </td>
                                    <td>@(Model.Grade)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Code:
                                    </td>
                                    <td>@(Model.Code)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Special Programs:
                                    </td>
                                    <td>@(Model.SpecialPrograms)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Home Phone:
                                    </td>
                                    <td>@(Model.HomePhone)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Mailing Address:
                                    </td>
                                    <td>@(Model.MailingAddress)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Mailing City:
                                    </td>
                                    <td>@(Model.MailingCity)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Mailing Province:
                                    </td>
                                    <td>@(Model.MailingProvince)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Mailing Postal Code:
                                    </td>
                                    <td>@(Model.PostalCode)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Address:
                                    </td>
                                    <td>@(Model.Address)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        City:
                                    </td>
                                    <td>@(Model.City)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Province:
                                    </td>
                                    <td>@(Model.Province)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Postal Code:
                                    </td>
                                    <td>@(Model.PostalCode)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Mother Name:
                                    </td>
                                    <td>@(Model.MotherName)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Mother Phone:
                                    </td>
                                    <td>@(Model.MotherPhone)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Mother Email:
                                    </td>
                                    <td>
                                        <a href="mailto:@(Model.MotherEmail)">@(Model.MotherEmail)</a>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Father Name:
                                    </td>
                                    <td>@(Model.FatherName)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Father Phone:
                                    </td>
                                    <td>@(Model.FatherPhone)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Father Email:
                                    </td>
                                    <td>
                                        <a href="mailto:@(Model.FatherEmail)">@(Model.FatherEmail)</a>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Guardian Name:
                                    </td>
                                    <td>@(Model.GuardianName)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Guardian Phone:
                                    </td>
                                    <td>@(Model.GuardianPhone)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Guardian Email:
                                    </td>
                                    <td>
                                        <a href="mailto:@(Model.GuardianEmail)">@(Model.GuardianEmail)</a>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Source ID:
                                    </td>
                                    <td>@(Model.SourceId)
                                    </td>
                                </tr>
                            </table>
                        </p>
                    </div>
                </div>
            </div>
            <!-- /content -->
        </div>
        <div class="clear">
        </div>
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
